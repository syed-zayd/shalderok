public class RockShooter extends Weapon {
    
    public RockShooter(Entity owner, double x, double y){
        super(owner, x, y, null);
    }

    public void shoot(){
        if(canShoot()){
            queuedProjectiles.add(new Rock(x, y, 3, angle, owner.damage));
            cooldownTimer = 200;
        }
    }

    @Override
    public void interact() {
        
    }

}
